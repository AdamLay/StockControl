<li>
    <div class="timeline-badge">
        <i class="fa"></i>
    </div>
    <div id="{{Id}}" class="timeline-panel">
        <div class="timeline-heading">
            <h4 class="timeline-title">{{Title}}</h4>
            <p>
                <small class="text-muted">{{Timestamp}}</small>
            </p>
        </div>
        <div class="timeline-body">
            <p>{{Message}}</p>
        </div>
    </div>
</li>